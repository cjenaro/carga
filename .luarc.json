{
    "runtime.version": "Lua 5.4",
    "runtime.path": [
        "?.lua",
        "?/init.lua",
        "src/?.lua",
        "src/?/init.lua"
    ],
    "workspace.library": [
        "src"
    ],
    "diagnostics.globals": [
        "describe",
        "it",
        "before_each",
        "after_each",
        "assert",
        "busted",
        "pending",
        "finally",
        "setup",
        "teardown",
        "insulate",
        "expose",
        "randomize",
        "spy",
        "stub",
        "mock",
        "match",
        "Model",
        "Database",
        "QueryBuilder",
        "Migration",
        "Associations",
        "Carga"
    ],
    "diagnostics.disable": [
        "unused-local",
        "unused-vararg",
        "trailing-space"
    ],
    "format.enable": true,
    "format.defaultConfig": {
        "indent_style": "space",
        "indent_size": "4",
        "max_line_length": "120"
    },
    "workspace.checkThirdParty": false
}